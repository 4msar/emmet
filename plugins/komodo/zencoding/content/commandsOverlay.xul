        oncommand="zen_controller.runAction('decrement_number_by_1');"/>
      <broadcaster
        id="cmd_zencoding_increment_01"
        key="key_cmd_zencoding_increment_01"
        desc="Zen Coding: Increment Number By 0.1"
        oncommand="zen_controller.runAction('increment_number_by_01');"/>
