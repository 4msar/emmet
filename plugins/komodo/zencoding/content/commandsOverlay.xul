        id="cmd_zencoding_split_join_tag"
        key="key_cmd_zencoding_split_join_tag"
        desc="Zen Coding: Split/Join Tag"
        oncommand="zen_controller.runAction('split_join_tag');"/>
      <broadcaster
        id="cmd_zencoding_toggle_comment"
        key="key_cmd_zencoding_toggle_comment"
